<rpc>
<load-configuration>
<configuration>
<security>
<nat>
<source>
<rule-set>
<name>%rule-set%</name>
<from><zone>%from-zone%</zone></from>
<to><zone>%to-zone%</zone></to>
<rule>
<name>%rule-name%</name>
<src-nat-rule-match>
<source-address>%private-subnet%</source-address>
</src-nat-rule-match>
<then>
<source-nat>
<pool><pool-name>%pool-name%</pool-name></pool>
</source-nat>
</then>
</rule>
</rule-set>
</source>
</nat>
</security>
</configuration>
</load-configuration>
</rpc>


